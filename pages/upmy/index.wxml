<view class="upmy_box">
  <view class="headimg" >
    <van-cell class="user">
      <view class="user_wb">
        <view class="van-cell-text">用户头像</view>
        <image class="uimg" src="{{userlist.avatarUrl}}"></image>
      </view>
    </van-cell>
    <van-cell title="用户名" value="{{userlist.nickName}}" />
    <van-cell title="性别" value="{{gender[userlist.gender]}}" />
    <van-cell title="地区" value="{{userlist.city}}" />
    <van-cell title="绑定的手机" value="" />
    <van-cell title="收获地址"  value="内容" />
    <button class="but" bindtap="delete">退出登录</button>
  </view>
</view>